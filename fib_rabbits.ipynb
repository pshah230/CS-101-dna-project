   "source": [
    "\n",
    "def fibonacci_rabbits(n, k):\n",
    "    num_parent = 1\n",
    "    num_child = 1\n",
    "    for i in range(n - 1):\n",
    "        num_child, num_parent = num_parent, num_parent + (num_child * k)\n",
    "    return num_child"
   ]
